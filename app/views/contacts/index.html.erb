<h1>Contacts</h1>
<% @contacts.each do |contact| %>

<li>
  <%=link_to contact.name, contact%> | <%=link_to 'Edit', edit_contact_path(contact)%> | <%=link_to "Delete", contact, method: :delete, data: {confirm: "Are you sure?"} %>
</li>
<% end %>
<li><%=link_to 'Create New Contact', new_contact_path%></li>
</ul>
